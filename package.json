{
	"name": "distr-ec2",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist && mkdir dist && npm run prestart",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "npm run start:dev",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"prestart": "cp -rv protos dist/protos",
		"prestart:dev": "cp -rv protos dist/protos",
		"db:sync": "npx mikro-orm schema:update --run",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"predev": "npm run generate",
		"generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated protos/main.proto --ts_proto_opt=nestJs=true -I ./protos"
	},
	"dependencies": {
		"@aws-sdk/client-ec2": "^3.53.0",
		"@grpc/grpc-js": "^1.5.4",
		"@grpc/proto-loader": "^0.6.9",
		"@mikro-orm/cli": "^4.5.10",
		"@mikro-orm/core": "^4.5.10",
		"@mikro-orm/mysql": "^4.5.10",
		"@mikro-orm/nestjs": "^4.3.1",
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^1.2.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/microservices": "^8.2.6",
		"@nestjs/platform-express": "^8.0.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"nanoid": "^3.3.1",
		"nanoid-dictionary": "^4.3.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0",
		"ts-proto": "^1.104.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.0.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.1",
		"@types/nanoid-dictionary": "^4.2.0",
		"@types/node": "^16.0.0",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.2.5",
		"prettier": "^2.3.2",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.3.5"
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "all"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"mikro-orm": {
		"useTsNode": true
	}
}
